{
	"builds": [
		{
			"src": "./build_files.sh",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "/staticfiles"
			}
		},
		{
			"src": "/Zions_AutoERP/wsgi.py",
			"use": "@vercel/python",
			"config": { "maxLambdaSize": "15mb" }
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/Zions_AutoERP/wsgi.py"
		},
		{
			"src": "/static/(.*)",
			"dest": "/static/$1"
		}
	]
}
